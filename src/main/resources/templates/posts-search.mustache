{{>layout/header}}

<table class="table table-horizontal table-bordered">
    <thead class="thead-strong">
    <tr>
        <th>글 번호</th>
        <th>카테고리</th>
        <th>제목</th>
        <th>작성자</th>
        <th>작성일</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#searchList}}
        <tr>
            <td>{{id}}</td>
            <td>{{category}}</td>
            <td><a href="/posts/view/{{id}}">{{title}}</a></td>
            <td>{{author}}</td>
            <td>{{createdDate}}</td>
        </tr>
    {{/searchList}}
    </tbody>
</table>

{{>layout/footer}}